<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="comtamuka120.httpsgithub.final_project">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/circlelogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".registration" />
        <activity
            android:name=".home_page"
            android:label="@string/title_activity_home_page"
            android:theme="@style/Theme.Design.NoActionBar" />
        <activity
            android:name=".profile"
            android:label="@string/title_activity_profile"
            android:theme="@style/Theme.Design.NoActionBar" />
        <activity
            android:name=".game_search"
            android:label="@string/title_activity_game_search"
            android:theme="@style/Theme.Design.NoActionBar"></activity>
    </application>

</manifest>